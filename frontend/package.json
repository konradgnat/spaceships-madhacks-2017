{
  "name": "phaser-es6-boilerplate",
  "version": "1.0.2",
  "description": "Tiny flexible template for writing Phaser games in ES6/ES2015 with Babel.",
  "main": "src/index.js",
  "scripts": {
    "start": "export NODE_ENV=\"development\" && node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --host 0.0.0.0 --content-base src --config=./webpack.config.dev.js --hot",
    "build-dev": "export NODE_ENV=\"development\" && webpack --config ./webpack.config.dev.js",
    "build-prod": "export NODE_ENV=\"production\" && webpack -p --config ./webpack.config.prod.js",
    "test": "echo \"Error: no test specified\" && exit s1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/belohlavek/phaser-es6-boilerplate"
  },
  "author": "Daniel Belohlavek",
  "license": "MIT",
  "devDependencies": {
    "babel-loader": "^6.3.2",
    "babel-preset-es2015": "^6.6.0",
    "babelify": "^7.2.0",
    "browser-sync": "^2.12.3",
    "browserify": "^13.0.0",
    "css-loader": "^0.26.1",
    "del": "^2.2.0",
    "exorcist": "^0.4.0",
    "gulp": "^3.9.1",
    "gulp-buffer": "0.0.2",
    "gulp-if": "^2.0.0",
    "gulp-uglify": "^1.5.3",
    "gulp-util": "^3.0.7",
    "html-webpack-plugin": "^2.28.0",
    "raw-loader": "^0.5.1",
    "style-loader": "^0.13.1",
    "vinyl-source-stream": "^1.1.0",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.3.0",
    "yargs": "^4.6.0"
  },
  "dependencies": {
    "jquery": "^3.1.1",
    "phaser": "^2.4.7"
  }
}
